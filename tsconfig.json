{
   "compilerOptions": {
   "target": "es5",
   "lib": ["es5", "es6", "dom", "dom.iterable"],
   "allowJs": true,
   "skipLibCheck": true,
   "strict": true,
   "forceConsistentCasingInFileNames": true,
   "noEmit": true,
   "esModuleInterop": true,
   "module": "esnext",
   "sourceMap": true,
   "moduleResolution": "node",
   "resolveJsonModule": true,
   "isolatedModules": true,
   "jsx": "preserve",
   "incremental": true
   },
   "include": ["next-env.d.ts", "/*.ts", "/*.tsx"],
   "exclude": ["node_modules"],
   "typeRoots" : [
      "./node_modules/@types",
      "./model/entities.tsx"
    ]
}

